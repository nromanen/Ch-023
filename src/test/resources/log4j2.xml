<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug">
    <!-- Appenders -->
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
                <PatternLayout pattern="%-5p: [%l] - %m%n"/>
        </Console>
    </Appenders>
        
    <Loggers>
        <Root>
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>
